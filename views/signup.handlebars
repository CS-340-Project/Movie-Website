<main id="signup-holder">
  <div id="signup-text">
    <form id="signup-form">
      <h1 id="signup-header">Sign Up</h1>
      <input type="text" name="username" id="signup-username-field" class="signup-form-field" placeholder="Username">
      <input type="password" name="password" id="signup-password-field" class="signup-form-field" placeholder="Password">
      <input type="submit" value="Sign Up" id="signup-form-submit">
    </form>
   </div>
  </main>
